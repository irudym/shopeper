{
    "extends": "airbnb",
    "plugins": [
        "react",
        "jsx-a11y",
        "import"
    ],
    "rules": {
      "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
      "jsx-a11y/no-static-element-interactions": 1,
      "max-len": 0
    },
    "globals": {
      "document": true,
      "window": true,
      "fetch": true,
      "FileReader": true,
      "$": true
    }
}
